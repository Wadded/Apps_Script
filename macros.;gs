function Conta_Format(){
  var format = Diario.getConditionalFormatRules();
  x=Diario.getConditionalFormatRules().length
  Logger.log(x)
  format.splice(1, x);
  Diario.setConditionalFormatRules(format);
}



function clearFormatValues() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('AR:AR').activate();
  spreadsheet.getActiveRangeList().clearFormat()
  .clear({contentsOnly: true, skipFilteredRows: true});
};

