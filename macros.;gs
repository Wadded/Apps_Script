function autoFill3() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('L160').activate();
  spreadsheet.getActiveRange().autoFill(spreadsheet.getRange('L150:L160'), SpreadsheetApp.AutoFillSeries.DEFAULT_SERIES);
  spreadsheet.getRange('N159:O159').activate();
  spreadsheet.getActiveRange().autoFill(spreadsheet.getRange('N147:O159'), SpreadsheetApp.AutoFillSeries.DEFAULT_SERIES);
  spreadsheet.getRange('R160').activate();
  spreadsheet.getActiveRange().autoFill(spreadsheet.getRange('R149:R160'), SpreadsheetApp.AutoFillSeries.DEFAULT_SERIES);
  spreadsheet.getRange('R149:R160').activate();
  spreadsheet.setCurrentCell(spreadsheet.getRange('R160'));
};


function formulas() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('J3').activate();
  spreadsheet.getCurrentCell().setFormula('=CONT.VALORES(Diário!D4:D17)');
  spreadsheet.getRange('J4').activate();
  spreadsheet.getCurrentCell().setFormula('=Diário!D95+Diário!X95');
  spreadsheet.getRange('J5').activate();
  spreadsheet.getCurrentCell().setFormula('=J4')
  .setFormula('=(J4*60)/1000');
  spreadsheet.getRange('J8').activate();
  spreadsheet.getCurrentCell().setFormula('=SOMASE(Diário!P4:P18;"Milho";Diário!I4:I18)');
  spreadsheet.getRange('I8').activate();
};

function setFormula1() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('J8').activate();
  spreadsheet.getCurrentCell().setFormula('=(SOMASE(Diario!P$4:P$93;B8;Diario!I$4:I$93))/($J$5*1000)');
  spreadsheet.getActiveRange().autoFill(spreadsheet.getRange('J8:J12'), SpreadsheetApp.AutoFillSeries.DEFAULT_SERIES);
  spreadsheet.getRange('J10').activate();


};
